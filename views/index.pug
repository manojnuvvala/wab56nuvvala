extends layout

block content
  h1= title
  p Live in the moment -#{title}
  button(type="button",onclick="update()" class="btn btn-dark") Click Here!!
  div(id="target") clickme
  script.
    var client_count=0;
    function update() {
      client_count++;
      document.getElementById("target").innerHTML=`Value now ${client_count}`
    }